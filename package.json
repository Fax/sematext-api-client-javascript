{
  "name": "sematext-api-client-javascript",
  "version": "0.1.0",
  "description": "Client library for https://sematext.com/docs/api/",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/sematext/sematext-api-client-javascript.git",
  "main": "stcloud/index.js",
  "type": "module",
  "scripts": {
    "test": "mocha --require babel-core/register --recursive",
    "standard": "standard lib/**/**/*.js bin/*.js",
    "format": "prettier-standard --format",
    "lint": "prettier-standard --format --lint '**/*.js'",
    "precommit": "npm run standard && npm run eslint",
    "auto-changelog": "auto-changelog",
    "release": "release-it"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "querystring": "^0.2.0",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "chai": "^4.2.0",
    "eslint-plugin-class-property": "^1.1.0",
    "expect.js": "^0.3.1",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prettier-standard": "^16.3.0",
    "release-it": "^13.6.2",
    "sinon": "^9.0.2",
    "standard": "^14.3.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseNotes": "npx auto-changelog --stdout --commit-limit false -u --template ./changelog.hbs"
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p"
    },
    "npm": {
      "ignoreVersion": true
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
